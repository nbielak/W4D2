<h1>All Cats</h1>

<a href="<%= new_cat_url %>">Create a Cat!</a>

<ul>
  <% @cats.each do |cat| %>
    <li>
        <ul>
          <li> <%= cat.id %> </li>
          <li><%= cat.name %></li>
          <li><%= cat.birth_date %></li>
          <li><%= cat.sex %></li>
          <li><%= cat.color %></li>
          <li><%= cat.age %></li>
          <li><%= cat.description %></li>
          <li><a href="<%= cat_url(cat) %>">Cat's show</a></li>
          <li><a href="<%= edit_cat_url(cat) %>">Update Cat</a></li>
        
        </ul>
    </li>
  <% end %>
</ul>
